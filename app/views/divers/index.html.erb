<% if diver_signed_in? %>
  <p>Welcome <%= current_diver.email %></p>
  <%= link_to 'Logged In [click to logout]', destroy_diver_session_path, :method => :delete %>
<% else %>
  <p>You are not signed in.</p>
  <div class="actions">
  <%= link_to 'Login', new_diver_session_path, id: "btn" %>
  </div>
<% end %>
