<h1> This is DiveType Index! </h1>

<% @dive_types.each do |dive_type| %>
<div class ="dive_type_wrapper">
  <h2 class = "Title"> <%= link_to dive_type.activity, controller:'dive_types', action:'show', id:dive_type.id %></h2>
  <p class = "date"> <%= dive_type.created_at.strftime("%B, %d, %Y") %></p>
</div>
<% end %>
